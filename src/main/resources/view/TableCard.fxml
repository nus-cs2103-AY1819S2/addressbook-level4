<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox id="tableCardPane" fx:id="tableCardPane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
        <VBox id="tableCard" alignment="CENTER_LEFT" minHeight="105" minWidth="100" maxWidth="100">
            <padding>
                <Insets bottom="5" left="15" right="15" top="5" />
            </padding>
            <Label fx:id="tableNumber" styleClass="cell_big_label" text="\$tableNumber" />
            <Label fx:id="tableStatus" styleClass="cell_small_label" text="\$tableStatus" />
        </VBox>
</HBox>
