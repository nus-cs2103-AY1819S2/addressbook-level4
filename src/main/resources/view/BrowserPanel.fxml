<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="browser" xmlns:fx="http://javafx.com/fxml/1">
  <VBox alignment="CENTER" minHeight="150">
    <padding>
      <Insets top="5" right="5" bottom="5" left="15" />
    </padding>
    <Label fx:id="currentBudget" styleClass="budget_title" text="\$currentBudget" />
    <Label fx:id="totalBudget" styleClass="budget_title" text="\$totalBudget" />
    <Label fx:id="currentSpending" styleClass="budget_title" text="\$currentSpending" />
  </VBox>
</StackPane>
