<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="imagePane" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane alignment="TOP_LEFT">
         <padding>
            <Insets bottom="3" left="6" right="6" top="6" />
         </padding>
         <children>
            <Text fx:id="commandName" fill="WHITE" text="COMMAND">
               <font>
                  <Font name="Gill Sans MT" size="18.0" />
               </font>
            </Text>
         </children>
      </StackPane>
      <StackPane alignment="TOP_LEFT">
         <padding>
            <Insets bottom="6" left="6" right="6" top="3" />
         </padding>
         <children>
            <Text fx:id="arguments" fill="WHITE" text="ARGUMENTS">
               <font>
                  <Font name="Gill Sans MT" size="14.0" />
               </font>
            </Text>
         </children>
      </StackPane>
   </children>
</VBox>
