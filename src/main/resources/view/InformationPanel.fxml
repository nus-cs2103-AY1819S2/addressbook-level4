<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<TabPane fx:id="informationPanel" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <tabs>
    <Tab text="Album Images">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
          <children>
            <ListView prefHeight="180.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                  <Text fx:id="arguments2" fill="WHITE" layoutX="14.0" layoutY="27.0" text="No data to display.">
                     <font>
                        <Font name="Gill Sans MT" size="14.0" />
                     </font>
                  </Text>
          </children>
        </AnchorPane>
      </content>
    </Tab>
    <Tab text="EXIF Profile">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">

          <children>
            <ListView prefHeight="180.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                  <Text fx:id="arguments1" fill="WHITE" layoutX="14.0" layoutY="27.0" text="No data to display.">
                     <font>
                        <Font name="Gill Sans MT" size="14.0" />
                     </font>
                  </Text>
          </children>
        </AnchorPane>
      </content>
    </Tab>
    <Tab text="Command History">
      <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
          <children>
            <ListView prefHeight="180.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                  <Text fx:id="arguments" fill="WHITE" layoutX="14.0" layoutY="27.0" text="No data to display.">
                     <font>
                        <Font name="Gill Sans MT" size="14.0" />
                     </font>
                  </Text>
          </children>
        </AnchorPane>
      </content>
    </Tab>
  </tabs>
</TabPane>
