<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.ScatterChart?>
<?import javafx.scene.layout.StackPane?>


<?import javafx.scene.layout.StackPane?>
<StackPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScatterChart fx:id="scatterChart" layoutX="-2.0" prefHeight="400.0" prefWidth="400.0" title="Salary">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" side="LEFT" upperBound="80000.0" />
        </yAxis>
      </ScatterChart>
   </children>
</StackPane>
