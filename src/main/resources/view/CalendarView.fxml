<VBox fx:id="vBoxCalendar" maxHeight="Infinity" maxWidth="Infinity" minHeight="-Infinity" minWidth="-Infinity"
      xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" VBox.vgrow="ALWAYS" HBox.hgrow="ALWAYS">

    <VBox BorderPane.alignment="CENTER" prefHeight="26" VBox.vgrow="NEVER" HBox.hgrow="ALWAYS">
        <children>
            <HBox prefHeight="26.0">
                <padding>
                    <Insets right="5" left="5" />
                </padding>
                <children>
                    <Label fx:id="monthLabel" prefWidth="150.0" />
                    <Label fx:id="yearLabel" prefWidth="150.0" />
